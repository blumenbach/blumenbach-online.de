$jscript_special_inputfields = '
<script language="javascript">

function autofill(tagidmaster,tagidaddon,bez,unit){
//	alert("agid: "+document.getElementById(tagidmaster).value);
	var master = document.getElementById(tagidmaster).value;
	var addon = document.getElementById(tagidaddon).value;
	addon = addon.replace(/^\s+|\s+$/g, "");
	addon = addon.replace(/^\s+|\s+$/g, "");
	addon = addon.replace(/,/g, ".");
		
	var literal = false;
	if(isNaN(parseFloat(addon))){ //Buchstabe uebergeben
		literal = true;
	} else {	
		addon = parseFloat(addon);
	}
	addon = addon==""?0:addon;
//alert("addon:"+addon+"literal:"+literal);
//if(isNaN(addon) || literal){ 
	if((!literal) && isNaN(addon)) {
		alert("Please give me a valid float value!"); return "";
	}
	//if(addon==0 && (unit!="\'\'")) {
	//	alert("Please give me a not empty value!"); return "";
	//}
	
	//if(addon!=""){
		var value = "";
		if(bez!="") {
			value = master+bez+"="+addon+" ["+unit+"]"+"\n";
			//alert("V1"+value);
		} else {
		   value = master+bez+addon+unit+" ";
		   //alert("V2"+value);
		}
		//alert(value);
		document.getElementById(tagidmaster).value = value;
	//} 
}

function autofillDate(tagidmaster,tagidaddon,type,trenner){
	//alert("agid: "+document.getElementById(tagidmaster).value);
	var master = document.getElementById(tagidmaster).value;
	var addon = document.getElementById(tagidaddon).value;
	addon = addon.replace(/^\s+|\s+$/g, "");
	addon = addon.replace(/^\s+|\s+$/g, "");
	addon = addon.replace(/,/g, ".");
	
	var literal = false;
	if(isNaN(parseFloat(addon))){ //Buchstabe uebergeben
		literal = true;
	} else {	
		addon = parseFloat(addon);
	}
//alert("addon:"+addon+"literal:"+literal);
//if(isNaN(addon) || literal){ 
	if((!literal) && isNaN(addon)) {
		alert("Please give me a valid float value!"); return "";
	}
	
	
	if(type=="J"){
	/*
		if((""+addon).length <4 || addon==0){
			addon="";
			alert("Wrong data: invalid year-format. Resetting data!");
			document.getElementById(tagidmaster).value = "";
			return;
		}
		*/
		while(addon.length <4) {
			addon = "0"+addon;
		}
		
	}
	
	if(type=="M"){
		if(addon >59 || addon==0){
			addon="";
			alert("Wrong data: invalid month-format. Resetting data!");
			document.getElementById(tagidmaster).value = "";
			return;
		}
		if(addon<10) addon = "0"+addon;
		
	}
	
	if(type=="D"){
		if(addon >31 || addon==0){
		 addon="";
		 alert("Wrong data: invalid day-format. Resetting data!");
		 document.getElementById(tagidmaster).value = "";
		 return;
		}
		if(addon<10) addon = "0"+addon;
		
	}
		
	
	var value = "";
	if(addon!="") {	
	   value = master+addon+trenner;
	}
	//alert(value);
	document.getElementById(tagidmaster).value = value;

}

function makeblank(elementid){ 
	document.getElementById(elementid).focus();
	document.getElementById(elementid).select();
	document.getElementById(elementid).value="";
}
</script>';


