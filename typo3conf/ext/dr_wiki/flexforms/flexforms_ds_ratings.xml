<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>0</langChildren>
	</meta>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:dr_wiki/flexforms/locallang_db_ratings.xml:tt_content.pi_flexform.ratingsConfig</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<enableRatings>
				<TCEforms>
				<label>LLL:EXT:dr_wiki/flexforms/locallang_db_ratings.xml:tt_content.pi_flexform.enableRatings</label>
				<displayCond>EXT:ratings:LOADED:true</displayCond>
				<config>
					<type>check</type>
					<default></default>
				</config>
				</TCEforms>
			</enableRatings>
			<enableRatingsPlugin>
				<TCEforms>
				<label>LLL:EXT:dr_wiki/flexforms/locallang_db_ratings.xml:tt_content.pi_flexform.enableRatingsPlugin</label>
				<displayCond>EXT:ratings:LOADED:true</displayCond>
				<config>
					<type>check</type>
					<default></default>
				</config>
				</TCEforms>
			</enableRatingsPlugin>
			<enableMostRatedPlugin>
				<TCEforms>
				<label>LLL:EXT:dr_wiki/flexforms/locallang_db_ratings.xml:tt_content.pi_flexform.enableMostRatedPlugin</label>
				<displayCond>EXT:ratings:LOADED:true</displayCond>
				<config>
					<type>check</type>
					<default></default>
				</config>
				</TCEforms>
			</enableMostRatedPlugin>
			<ratingsStoragePid>
				<TCEforms>
					<label>LLL:EXT:ratings/pi1/locallang.xml:tt_content.tx_ratings_pi1.storage.page</label>
					<displayCond>EXT:ratings:LOADED:true</displayCond>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<prepand_tname>0</prepand_tname>
						<multiple>0</multiple>
						<minitems>0</minitems>
						<maxitems>1</maxitems>
						<size>1</size>
					</config>
				</TCEforms>
			</ratingsStoragePid>
			<ratingsTemplateFile>
				<TCEforms>
					<label>LLL:EXT:ratings/pi1/locallang.xml:tt_content.tx_ratings_pi1.template_file</label>
					<displayCond>EXT:ratings:LOADED:true</displayCond>
					<config>
						<type>input</type>
						<eval>trim,required</eval>
						<default>EXT:ratings/res/ratings.html</default>
						<wizards type="array">
							<_PADDING>2</_PADDING>
							<link type="array">
								<type>popup</type>
								<title>Link</title>
								<icon>link_popup.gif</icon>
								<script>browse_links.php?mode=wizard&amp;act=file</script>
								<params type="array">
									<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
								</params>
								<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
							</link>
						</wizards>
					</config>
				</TCEforms>
			</ratingsTemplateFile>
		</el>
	</ROOT>
</T3DataStructure>